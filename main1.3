void bubble_sort(int *vet){
int i, j, aux;

for(j=999;j>=0;j--){
	for(i=0;i<j;i++){
		if(vet[i]>vet[i+1]){
			aux=vet[i];
			vet[i]=vet[i+1];
			vet[i+1]=aux;
		}
	}
}

}	

